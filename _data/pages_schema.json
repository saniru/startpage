{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9 ]+$": {
      "type": "object",
      "properties": {
        "link": {
          "type": "string",
          "format": "uri"
        },
        "icon": {
          "type": "string"
        }
      },
      "required": ["link", "icon"],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
